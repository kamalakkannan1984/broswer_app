<span class="dropdown-menu-arrow"></span>
<a class="dropdown-item" href="#">
    <ul class="list-unstyled dial-name">
        {{#each this}}
        <li class="media search-call-bind" bid={{ext}}>
            {{#if ImageURL}}
            <img class="mr-3" src={{ImageURL}} alt={{caller_id}}>
            {{else}}
            <img class="mr-3" src="images/pho.png" alt="Generic placeholder image">
            {{/if}}
            <div class="media-body">
                <h5 class="mt-0"><span class="blue-clr">{{caller_id}}</h5>
                <P> Ext. {{ext}}</P>
            </div>
        </li>
        {{/each}}
    </ul>
</a>