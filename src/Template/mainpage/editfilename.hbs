<div class="modal" id="editFileName" role="dialog">
		<div class="modal-dialog editors">    
			
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal"><img src="images/close.svg"/></button>
					<h4 class="modal-title">Edit File</h4>
				</div>
				<div class="modal-body">
					<div class="row clearfix">
							<input id="compose_file_filename" name="filename" type="text" placeholder="Filename" autocomplete="off" maxlength="28">
					</div>
					
				</div>
				<div class="modal-footer">
				
					<button type="button" class="btn btn-default closes" data-dismiss="modal">Discard</button>
					<button type="button" class="btn btn-default proceed" data-dismiss="modal">Save</button>
				</div>
			</div>      
		</div>
	</div>